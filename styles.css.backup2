}

.journey-line {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 2px;
    height: 100%;
    background: linear-gradient(
        to bottom,
        transparent,
        rgba(255, 255, 255, 0.3) 10%,
        rgba(255, 255, 255, 0.3) 90%,
        transparent
