}

.journey-item {
    position: relative;
    margin: 4rem 0;
    opacity: 0;
    transform: translateY(30px);
    transition: all 0.8s cubic-bezier(0.23, 1, 0.32, 1);
}

.journey-item.visible {
    opacity: 1;
