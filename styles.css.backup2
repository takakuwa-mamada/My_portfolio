/* ===== JOURNEY ÁπßÔΩªÁπßÔΩØÁπßÔΩ∑ÁπùÔΩßÁπùÔΩ≥ ===== */
.journey {
    min-height: 100vh;
    padding: 8rem 0;
    position: relative;
}

.journey .section-subtitle {
    text-align: center;
    margin-top: 1rem;
    font-size: 1.1rem;
    opacity: 0.7;
    font-weight: 300;
}

.journey-timeline {
    position: relative;
    max-width: 1200px;
    margin: 6rem auto 8rem;
    padding: 2rem;
}

.journey-line {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 2px;
    height: 100%;
    background: linear-gradient(
        to bottom,
        transparent,
        rgba(255, 255, 255, 0.3) 10%,
        rgba(255, 255, 255, 0.3) 90%,
        transparent
    );
}

.journey-item {
    position: relative;
    margin: 4rem 0;
    opacity: 0;
    transform: translateY(30px);
    transition: all 0.8s cubic-bezier(0.23, 1, 0.32, 1);
}

.journey-item.visible {
    opacity: 1;
    transform: translateY(0);
}

.journey-year {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 0;
    font-size: 1.5rem;
    font-weight: 700;
    color: var(--color-white);
    background: var(--color-black);
    padding: 0.5rem 1.5rem;
    border: 2px solid var(--color-white);
    z-index: 10;
    font-family: 'JetBrains Mono', monospace;
}

.journey-card {
    background: rgba(255, 255, 255, 0.02);
    border: 1px solid rgba(255, 255, 255, 0.1);
    padding: 3rem;
    margin: 0 0 0 55%;
    position: relative;
    transition: all 0.4s ease;
}

.journey-item:nth-child(even) .journey-card {
    margin: 0 55% 0 0;
}

.journey-card::before {
    content: '';
    position: absolute;
    top: 2rem;
    width: 40px;
    height: 2px;
    background: rgba(255, 255, 255, 0.3);
}

.journey-item:nth-child(odd) .journey-card::before {
    left: -40px;
}

.journey-item:nth-child(even) .journey-card::before {
    right: -40px;
}

.journey-card:hover {
    background: rgba(255, 255, 255, 0.05);
    border-color: rgba(255, 255, 255, 0.3);
    transform: translateY(-5px);
    box-shadow: 0 10px 30px rgba(255, 255, 255, 0.1);
}

.journey-card.active {
    border-color: rgba(255, 255, 255, 0.5);
    box-shadow: 0 0 30px rgba(255, 255, 255, 0.2);
}

.journey-card.future {
    border-style: dashed;
    opacity: 0.7;
}

.journey-icon {
    width: 60px;
    height: 60px;
    background: rgba(255, 255, 255, 0.05);
    border: 2px solid rgba(255, 255, 255, 0.3);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.5rem;
    margin-bottom: 1.5rem;
}

.journey-title {
    font-size: 1.8rem;
    font-weight: 600;
    margin-bottom: 0.5rem;
}

.journey-subtitle {
    font-size: 1.1rem;
    color: rgba(255, 255, 255, 0.7);
    margin-bottom: 1.5rem;
    font-family: 'JetBrains Mono', monospace;
}

.journey-description {
    margin-bottom: 1.5rem;
}

.journey-description p {
    line-height: 1.8;
    margin-bottom: 1rem;
}

.journey-achievements {
    list-style: none;
    padding: 0;
    margin: 1rem 0;
}

.journey-achievements li {
    padding: 0.5rem 0;
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

.journey-achievements i {
    color: rgba(255, 255, 255, 0.8);
}

.journey-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 0.5rem;
    margin-top: 1.5rem;
}

.journey-tags span {
    padding: 0.4rem 1rem;
    background: rgba(255, 255, 255, 0.05);
    border: 1px solid rgba(255, 255, 255, 0.2);
    font-size: 0.85rem;
    font-family: 'JetBrains Mono', monospace;
}

.journey-badge {
    position: absolute;
    top: -15px;
    right: 20px;
    background: var(--color-white);
    color: var(--color-black);
    padding: 0.3rem 1rem;
    font-size: 0.9rem;
    font-weight: 700;
    font-family: 'JetBrains Mono', monospace;
    animation: pulse-badge 2s ease-in-out infinite;
}

.future-badge {
    background: transparent;
    color: var(--color-white);
    border: 2px solid rgba(255, 255, 255, 0.5);
    animation: glow-badge 2s ease-in-out infinite;
}

@keyframes pulse-badge {
    0%, 100% {
        box-shadow: 0 0 10px rgba(255, 255, 255, 0.5);
    }
    50% {
        box-shadow: 0 0 20px rgba(255, 255, 255, 0.8);
    }
}

@keyframes glow-badge {
    0%, 100% {
        box-shadow: 0 0 10px rgba(255, 255, 255, 0.3);
    }
    50% {
        box-shadow: 0 0 20px rgba(255, 255, 255, 0.6);
    }
}

