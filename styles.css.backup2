/* ===== Áã¨ÂâµÁöÑ„Å™ËøΩÂä†„Ç®„Éï„Çß„ÇØ„Éà ===== */

/* „Ç∞„É™„ÉÉ„ÉÅ„ÉÜ„Ç≠„Çπ„Éà„Ç®„Éï„Çß„ÇØ„Éà */
.hero-title .title-line {
    position: relative;
    display: inline-block;
}

.hero-title .title-line::before,
.hero-title .title-line::after {
    content: attr(data-text);
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0.8;
}

.hero-title .title-line::before {
    animation: glitch-anim-1 2s infinite;
    color: rgba(255, 255, 255, 0.5);
    z-index: -1;
}

.hero-title .title-line::after {
    animation: glitch-anim-2 3s infinite;
    color: rgba(255, 255, 255, 0.3);
    z-index: -2;
}

@keyframes glitch-anim-1 {
    0%, 100% { transform: translate(0); }
    20% { transform: translate(-2px, 2px); }
    40% { transform: translate(-2px, -2px); }
    60% { transform: translate(2px, 2px); }
    80% { transform: translate(2px, -2px); }
}

@keyframes glitch-anim-2 {
    0%, 100% { transform: translate(0); }
    20% { transform: translate(2px, -2px); }
    40% { transform: translate(2px, 2px); }
    60% { transform: translate(-2px, -2px); }
    80% { transform: translate(-2px, 2px); }
}

/* „Çπ„Ç≠„É£„É≥„É©„Ç§„É≥„Ç®„Éï„Çß„ÇØ„ÉàÔºàÂÖ®ÁîªÈù¢Ôºâ */
body::before {
    content: '';
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(
        to bottom,
        transparent 50%,
        rgba(255, 255, 255, 0.02) 50%
    );
    background-size: 100% 4px;
    pointer-events: none;
    z-index: 9998;
    animation: scanline 10s linear infinite;
}

@keyframes scanline {
    0% { transform: translateY(0); }
    100% { transform: translateY(100%); }
}

/* „Éç„Ç™„É≥„Ç∞„É≠„ÉºÂäπÊûú */
.btn-cyber {
    position: relative;
    box-shadow: 0 0 10px rgba(255, 255, 255, 0.1);
    transition: all 0.3s ease;
}

.btn-cyber:hover {
    box-shadow: 0 0 20px rgba(255, 255, 255, 0.3),
                0 0 40px rgba(255, 255, 255, 0.1);
    transform: translateY(-2px);
}

/* „Éõ„É≠„Ç∞„É©„É†„Ç®„Éï„Çß„ÇØ„Éà */
.project-card::before {
    content: '';
    position: absolute;
    top: -2px;
    left: -2px;
    right: -2px;
    bottom: -2px;
    background: linear-gradient(45deg, 
        transparent 25%, 
        rgba(255, 255, 255, 0.05) 25%, 
        rgba(255, 255, 255, 0.05) 50%, 
        transparent 50%, 
        transparent 75%, 
        rgba(255, 255, 255, 0.05) 75%);
    background-size: 20px 20px;
    opacity: 0;
    transition: opacity 0.3s ease;
    pointer-events: none;
}

.project-card:hover::before {
    opacity: 1;
    animation: hologram 20s linear infinite;
}

@keyframes hologram {
    0% { background-position: 0 0; }
    100% { background-position: 400px 400px; }
}

/* „Éá„Éº„Çø„Çπ„Éà„É™„Éº„É†„Ç®„Éï„Çß„ÇØ„Éà */
.section-title::before {
    content: '';
    margin-right: 1rem;
    opacity: 0.5;
    animation: blink-cursor 1s step-end infinite;
}

@keyframes blink-cursor {
    0%, 100% { opacity: 1; }
    50% { opacity: 0; }
}

/* ÊµÆÈÅä„Ç¢„Éã„É°„Éº„Ç∑„Éß„É≥ */
.skill-block {
    animation: float 6s ease-in-out infinite;
}

.skill-block:nth-child(2) {
    animation-delay: -2s;
}

.skill-block:nth-child(3) {
    animation-delay: -4s;
}

@keyframes float {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

/* „Éë„É´„Çπ„Ç®„Éï„Çß„ÇØ„Éà */
.timeline-marker {
    animation: pulse 2s ease-in-out infinite;
}

@keyframes pulse {
    0%, 100% { 
        box-shadow: 0 0 0 0 rgba(255, 255, 255, 0.7);
    }
    50% { 
        box-shadow: 0 0 0 10px rgba(255, 255, 255, 0);
    }
}

/* „Éó„É≠„Éï„Ç£„Éº„É´„Çπ„Ç≠„É£„É≥„Ç®„Éï„Çß„ÇØ„ÉàÊîπËâØ */
.profile-scan {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 3px;
    background: linear-gradient(90deg, 
        transparent, 
        rgba(255, 255, 255, 0.8), 
        transparent);
    box-shadow: 0 0 10px rgba(255, 255, 255, 0.8);
    animation: scan 3s ease-in-out infinite;
}

@keyframes scan {
    0%, 100% {
        top: 0;
        opacity: 0;
    }
    10% {
        opacity: 1;
    }
    90% {
        opacity: 1;
    }
    100% {
        top: 100%;
        opacity: 0;
    }
}

/* 3DÂõûËª¢„Ç®„Éï„Çß„ÇØ„ÉàÂº∑Âåñ */
.project-card {
    transform-style: preserve-3d;
    transition: transform 0.6s cubic-bezier(0.23, 1, 0.32, 1);
}

/* „Ç≥„Éº„Éä„Éº„Ç¢„ÇØ„Çª„É≥„Éà */
.profile-border::before,
.profile-border::after {
    content: '';
    position: absolute;
    width: 20px;
    height: 20px;
    border: 2px solid var(--color-white);
}

.profile-border::before {
    top: -2px;
    left: -2px;
    border-right: none;
    border-bottom: none;
    animation: corner-glow 2s ease-in-out infinite;
}

.profile-border::after {
    bottom: -2px;
    right: -2px;
    border-left: none;
    border-top: none;
    animation: corner-glow 2s ease-in-out infinite;
    animation-delay: 1s;
}

@keyframes corner-glow {
    0%, 100% {
        box-shadow: 0 0 5px rgba(255, 255, 255, 0.5);
    }
    50% {
        box-shadow: 0 0 15px rgba(255, 255, 255, 1);
    }
}

/* „Çø„Ç§„Éî„É≥„Ç∞„Ç®„Éï„Çß„ÇØ„ÉàÊîπËâØ */
.typewriter {
    overflow: hidden;
    border-right: 3px solid var(--color-white);
    white-space: nowrap;
    animation: 
        typing 3s steps(40, end) 1s,
        blink-caret 0.75s step-end infinite;
}

@keyframes typing {
    from { width: 0; }
    to { width: 100%; }
}

@keyframes blink-caret {
    50% { border-color: transparent; }
}

/* „Éä„Éì„Ç≤„Éº„Ç∑„Éß„É≥„É™„É≥„ÇØÊîπËâØ */
.nav-link {
    position: relative;
    overflow: hidden;
}

.nav-link::before {
    content: '';
    position: absolute;
    bottom: -2px;
    left: 0;
    width: 100%;
    height: 1px;
    background: linear-gradient(90deg, 
        transparent, 
        var(--color-white), 
        transparent);
    transform: translateX(-100%);
    transition: transform 0.3s ease;
}

.nav-link:hover::before {
    transform: translateX(100%);
}

/* „Éï„Ç©„Éº„É†ÂÖ•Âäõ„Éï„Ç©„Éº„Ç´„Çπ„Ç®„Éï„Çß„ÇØ„Éà */
.form-group {
    position: relative;
}

.form-group::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 0;
    height: 2px;
    background: linear-gradient(90deg, 
        var(--color-white), 
        transparent);
    transition: width 0.3s ease;
}

.form-group:focus-within::after {
    width: 100%;
}

/* „Çπ„ÇØ„É≠„Éº„É´ÈÄ≤Êçó„Éê„ÉºÊîπËâØ */
.scroll-progress {
    position: fixed;
    top: 0;
    left: 0;
    width: 0;
    height: 3px;
    background: linear-gradient(90deg, 
        var(--color-white), 
        rgba(255, 255, 255, 0.5));
    z-index: 10000;
    box-shadow: 0 0 10px rgba(255, 255, 255, 0.5);
}

/* „Çª„ÇØ„Ç∑„Éß„É≥Áï™Âè∑Âº∑Ë™ø */
.section-number {
    position: relative;
}

.section-number::after {
    content: '';
    position: absolute;
    top: 50%;
    right: -10px;
    width: 50px;
    height: 1px;
    background: linear-gradient(90deg, 
        var(--color-white), 
        transparent);
    transform: translateY(-50%);
}

/* Áµ±Ë®àÊï∞Â≠ó„Ç¢„Éã„É°„Éº„Ç∑„Éß„É≥Âº∑Âåñ */
.stat-number {
    position: relative;
    text-shadow: 0 0 10px rgba(255, 255, 255, 0.5);
}

/* „É¨„Çπ„Éù„É≥„Ç∑„ÉñË™øÊï¥ */
@media (max-width: 768px) {
    body::before {
        display: none;
    }
    
